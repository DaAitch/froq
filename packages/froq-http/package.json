{
  "name": "froq-http",
  "version": "0.0.8",
  "description": "Integration Testing environment orchestration made easy for HTTP.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d ./lib src",
    "build:debug": "babel -s -d ./lib src",
    "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 ava test/*.test.js --verbose",
    "clean": "rm -rf lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaAitch/froq"
  },
  "keywords": [
    "test",
    "orchestration",
    "integration",
    "http"
  ],
  "author": "Philipp Renoth <renoth@wildoak.de>",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0",
    "froq-util": "^0.0.8",
    "http-proxy": "^1.16.2",
    "mime-types": "^2.1.17"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "node-fetch": "^1.7.3"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit",
    "failFast": true
  }
}
