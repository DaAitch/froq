{
  "name": "froq-docker",
  "version": "0.0.16",
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.10",
    "npm": ">=3.10.10"
  },
  "scripts": {
    "build": "babel -d ./lib src",
    "build:debug": "babel -s -d ./lib src",
    "test": "ava test/*.test.js --verbose",
    "test:serial": "ava test/*.test.js --verbose --serial",
    "clean": "rm -rf lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaAitch/froq"
  },
  "keywords": [
    "test",
    "orchestration",
    "integration",
    "docker"
  ],
  "author": "Philipp Renoth <renoth@wildoak.de>",
  "license": "MIT",
  "description": "Integration Testing environment orchestration made easy for Docker.",
  "dependencies": {
    "JSONStream": "^1.3.2",
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "froq-docker-util": "^0.0.15",
    "froq-util": "^0.0.15",
    "node-fetch": "^2.1.2"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit",
    "failFast": true
  }
}
