{
  "name": "froq-docker-util",
  "version": "0.0.9",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d ./lib src",
    "build:debug": "babel -s -d ./lib src",
    "test": "ava test/*.test.js --verbose",
    "test:serial": "ava test/*.test.js --verbose --serial",
    "clean": "rm -rf lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaAitch/froq"
  },
  "keywords": [
    "test",
    "orchestration",
    "integration",
    "docker",
    "docker-util"
  ],
  "author": "Philipp Renoth <renoth@wildoak.de>",
  "license": "MIT",
  "description": "Integration Testing environment orchestration made easy for Docker.",
  "dependencies": {
    "debug": "^3.1.0",
    "tar-stream": "^1.5.5"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "rimraf": "^2.6.2"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit",
    "failFast": true
  }
}
